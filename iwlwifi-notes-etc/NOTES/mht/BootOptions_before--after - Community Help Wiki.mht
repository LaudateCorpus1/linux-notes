//
// MoinMoin commonly used JavaScript functions
//

// We keep here the state of the search box
searchIsDisabled =3D false;

function searchChange(e) {
    // Update search buttons status according to search box content.
    // Ignore empty or whitespace search term.
    var value =3D e.value.replace(/\s+/, '');
    if (value =3D=3D '' || searchIsDisabled) {=20
        searchSetDisabled(true);
    } else {
        searchSetDisabled(false);
    }
}

function searchSetDisabled(flag) {
    // Enable or disable search
    document.getElementById('fullsearch').disabled =3D flag;
    document.getElementById('titlesearch').disabled =3D flag;
}

function searchFocus(e) {
    // Update search input content on focus
    if (e.value =3D=3D search_hint) {
        e.value =3D '';
        e.className =3D '';
        searchIsDisabled =3D false;
    }
}

function searchBlur(e) {
    // Update search input content on blur
    if (e.value =3D=3D '') {
        e.value =3D search_hint;
        e.className =3D 'disabled';
        searchIsDisabled =3D true;
    }
}

function actionsMenuInit(title) {
    // Initialize action menu
    for (i =3D 0; i < document.forms.length; i++) {
        var form =3D document.forms[i];
        if (form.className =3D=3D 'actionsmenu') {
            // Check if this form needs update
            var div =3D form.getElementsByTagName('div')[0];
            var label =3D div.getElementsByTagName('label')[0];
            if (label) {
                // This is the first time: remove label and do buton.
                div.removeChild(label);
                var dobutton =3D div.getElementsByTagName('input')[0];
                div.removeChild(dobutton);
                // and add menu title
                var select =3D div.getElementsByTagName('select')[0];
                var item =3D document.createElement('option');
                item.appendChild(document.createTextNode(title));
                item.value =3D 'show';
                select.insertBefore(item, select.options[0]);
                select.selectedIndex =3D 0;
            }
        }
    }
}

// use this instead of assigning to window.onload directly:
function addLoadEvent(func) {
  // alert("addLoadEvent " + func)
  var oldonload =3D window.onload;
  if (typeof window.onload !=3D 'function') {
    window.onload =3D func;
  } else {
    window.onload =3D function() {
      oldonload();
      func();
    }
  }
}

function can_use_gui_editor() {
    var sAgent =3D navigator.userAgent.toLowerCase() ;

    // Internet Explorer
    if ( sAgent.indexOf("msie") !=3D -1 && sAgent.indexOf("mac") =3D=3D -1 =
&& sAgent.indexOf("opera") =3D=3D -1 )
    {  =20
        var sBrowserVersion =3D navigator.appVersion.match(/MSIE (.\..)/)[1=
] ;
        return ( sBrowserVersion >=3D 5.5 ) ;
    }
   =20
    // Gecko
    if ( navigator.product =3D=3D "Gecko" && navigator.productSub >=3D 2003=
0210 )
        return true ;

    // Opera
    if ( this.EnableOpera )
    {  =20
        var aMatch =3D sAgent.match( /^opera\/(\d+\.\d+)/ ) ;
        if ( aMatch && aMatch[1] >=3D 9.0 )
            return true ;
    }
   =20
    // Safari
    if ( this.EnableSafari && sAgent.indexOf( 'safari' ) !=3D -1 )
        return ( sAgent.match( /safari\/(\d+)/ )[1] >=3D 312 ) ;  // Build =
must be at least 312 (1.3)

    return false ;

}


function update_edit_links() {
    // Update editlink according if if the browser is compatible
    if (can_use_gui_editor() =3D=3D false){
        //alert("update_edit_links: can't use gui editor");
        return;
    }
    var editlinks =3D document.getElementsByName("editlink");
    for (i =3D 0; i < editlinks.length; i++) {
        var link =3D editlinks[i];
        href =3D link.href.replace('editor=3Dtextonly','editor=3Dguipossibl=
e');
        link.href =3D href;
        //alert("update_edit_links: modified to guipossible");
    }
}


function add_gui_editor_links() {
    // Add gui editor link after the text editor link
   =20
    // If the variable is not set or browser is not compatible, exit
    try {gui_editor_link_href}
    catch (e) {
        //alert("add_gui_editor_links: gui_editor_link_href not here");
        return
    }
    if (can_use_gui_editor() =3D=3D false){
        //alert("add_gui_editor_links: can't use gui_editor");
        return;
    }
    var all =3D document.getElementsByName('texteditlink');
    for (i =3D 0; i < all.length; i++) {
        var textEditorLink =3D all[i];
        // Create a list item with a link
        var guiEditorLink =3D document.createElement('a');
        guiEditorLink.href =3D gui_editor_link_href;
        var text =3D document.createTextNode(gui_editor_link_text);
        guiEditorLink.appendChild(text);
        var listItem =3D document.createElement('li')
        listItem.appendChild(guiEditorLink);
        // Insert in the editbar
        var editbar =3D textEditorLink.parentNode.parentNode
        var nextListItem =3D textEditorLink.parentNode.nextSibling;
        editbar.insertBefore(listItem, nextListItem);
        //alert("add_gui_editor_links: added gui editor link");
    }
}
=20

function show_switch2gui() {
    // Show switch to gui editor link if the browser is compatible
    if (can_use_gui_editor() =3D=3D false) return;
   =20
    var switch2gui =3D document.getElementById('switch2gui')
    if (switch2gui) {
        switch2gui.style.display =3D 'inline';
    }
}

function toggleComments() {
    // Toggle visibility of every tag with class =3D=3D *comment*
    var all =3D document.getElementsByTagName('*');
    for (i =3D 0; i < all.length; i++){
        el =3D all[i];
        if ( el.className.indexOf('comment') >=3D 0 ){
            if ( el.style.display !=3D 'none' ) {
                el.style.display =3D 'none';
            } else {
                el.style.display =3D '';
            }
        }
    }
}

function show_toggleComments() {
    // Show edit bar item "ToggleComments" if inline comments exist on this=
 page
    var all =3D document.getElementsByTagName('*');
    var count =3D 0;
    for (i =3D 0; i < all.length; i++){
        el =3D all[i];
        if ( el.className.indexOf('comment') >=3D 0 ){
            count++;
        }
    }
    if (count > 0) {
        for (i =3D 0; i < all.length; i++){
            el =3D all[i];
            if ( el.className =3D=3D 'toggleCommentsButton' ){
                el.style.display =3D 'inline';
            }
        }
    }
}


function load() {
    // Do not name this "onload", it does not work with IE :-)
    // TODO: create separate onload for each type of view and set the
    // correct function name in the html.=20
    // e.g <body onlod=3D'editor_onload()'>

    // login focus
    if (document.forms['loginform']) {
    	document.forms['loginform'].elements['name'].focus();
    }
   =20
    // Page view stuff
    update_edit_links();
    add_gui_editor_links();
   =20
    // Editor stuff
    show_switch2gui();

    // Disable menu item "ToggleComments" if no inline comments exist
    show_toggleComments();
}


function before_unload(evt) {
    // TODO: Better to set this in the editor html, as it does not make
    // sense elsehwere.
    // confirmleaving is available when editing
    try {return confirmleaving();}
    catch (e) {}
}

// Initialize after loading the page
addLoadEvent(load)

// Catch before unloading the page
window.onbeforeunload =3D before_unload


