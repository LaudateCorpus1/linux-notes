und: #ddddff; }
.navbox-subgroup .navbox-group, .navbox-subgroup .navbox-abovebelow { backg=
round: #e6e6ff; }
.navbox-even { background: #f7f7f7; }
.navbox-odd { background: transparent; }

/* Navigation Tabs */
.navtabs .tabs li { list-style:none; }
.navtabs .tabs a { text-decoration:none; text-transform:uppercase; outline-=
width:0px; font-size:x-small; font-weight:bold; color:black; }=20
