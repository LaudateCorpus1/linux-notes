an> <span class=3D"n">argv</span><span class=3D"p">)</span>
<span class=3D"p">{</span>
        <span class=3D"k">struct</span> <span class=3D"n">termios</span> <s=
pan class=3D"n">tio</span><span class=3D"p">;</span>
        <span class=3D"k">struct</span> <span class=3D"n">termios</span> <s=
pan class=3D"n">stdio</span><span class=3D"p">;</span>
        <span class=3D"k">struct</span> <span class=3D"n">termios</span> <s=
pan class=3D"n">old_stdio</span><span class=3D"p">;</span>
        <span class=3D"kt">int</span> <span class=3D"n">tty_fd</span><span =
class=3D"p">;</span>

        <span class=3D"kt">unsigned</span> <span class=3D"kt">char</span> <=
span class=3D"n">c</span><span class=3D"o">=3D</span><span class=3D"sc">&#x=
27;D&#x27;</span><span class=3D"p">;</span>
