    <!-- setup -->
    <a name=3D"setup"></a>
    <div class=3D"scrollblock block-setup">
        <h2>setup</h2>
        <p>
            <a href=3D"http://code.google.com/p/git-osx-installer/downloads=
/list?can=3D3">Download git for OSX</a>
        </p>
        <p>
