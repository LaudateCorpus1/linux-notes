/**
 * selection prototype
 *
 * Object that capsulates the selection in a textarea. Returned by DWgetSel=
ection.
 *
 * @author Andreas Gohr <andi@splitbrain.org>
 */
function selection_class(){
    this.start     =3D 0;
    this.end       =3D 0;
    this.obj       =3D null;
    this.rangeCopy =3D null;
    this.scroll    =3D 0;
    this.fix       =3D 0;

    this.getLength =3D function(){
        return this.end - this.start;
    };

    this.getText =3D function(){
        return (!this.obj) ? '' : this.obj.value.substring(this.start,this.=
end);
    };
}

/**
 * Get current selection/cursor position in a given textArea
 *
 * @link   http://groups.drupal.org/node/1210
 * @author Andreas Gohr <andi@splitbrain.org>
 * @link   http://linebyline.blogspot.com/2006/11/textarea-cursor-position-=
in-internet.html
 * @returns object - a selection object
 */
function DWgetSelection(textArea) {
    var sel =3D new selection_class();

    sel.obj   =3D textArea;
    sel.start =3D textArea.value.length;
    sel.end   =3D textArea.value.length;
    textArea.focus();
    if(document.getSelection) {          // Mozilla et al.
        sel.start  =3D textArea.selectionStart;
        sel.end    =3D textArea.selectionEnd;
        sel.scroll =3D textArea.scrollTop;
    } else if(document.selection) {      // MSIE
        /*
         * This huge lump of code is neccessary to work around two MSIE bug=
s:
         *
         * 1. Selections trim newlines at the end of the code
         * 2. Selections count newlines as two characters
         */

        // The current selection
        sel.rangeCopy =3D document.selection.createRange().duplicate();
        if (textArea.tagName =3D=3D=3D 'INPUT')  {
            var before_range =3D textArea.createTextRange();
            before_range.expand('textedit');                       // Selec=
ts all the text
        } else {
            var before_range =3D document.body.createTextRange();
            before_range.moveToElementText(textArea);              // Selec=
ts all the text
        }
        before_range.setEndPoint("EndToStart", sel.rangeCopy);     // Moves=
 the end where we need it

        var before_finished =3D false, selection_finished =3D false;
        var before_text, selection_text;
        // Load the text values we need to compare
        before_text  =3D before_range.text;
        selection_text =3D sel.rangeCopy.text;

        sel.start =3D before_text.length;
        sel.end   =3D sel.start + selection_text.length;

        // Check each range for trimmed newlines by shrinking the range by =
1 character and seeing
        // if the text property has changed.  If it has not changed then we=
 know that IE has trimmed
        // a \r\n from the end.
        do {
            if (!before_finished) {
                if (before_range.compareEndPoints("StartToEnd", before_rang=
e) =3D=3D 0) {
                    before_finished =3D true;
                } else {
                    before_range.moveEnd("character", -1);
                    if (before_range.text =3D=3D before_text) {
                        sel.start +=3D 2;
                        sel.end +=3D 2;
                    } else {
                        before_finished =3D true;
                    }
                }
            }
            if (!selection_finished) {
                if (sel.rangeCopy.compareEndPoints("StartToEnd", sel.rangeC=
opy) =3D=3D 0) {
                    selection_finished =3D true;
                } else {
                    sel.rangeCopy.moveEnd("character", -1);
                    if (sel.rangeCopy.text =3D=3D selection_text) {
                        sel.end +=3D 2;
                    } else {
                        selection_finished =3D true;
                    }
                }
            }
        } while ((!before_finished || !selection_finished));

        // count number of newlines in str to work around stupid IE selecti=
on bug
        var countNL =3D function(str) {
            var m =3D str.split("\r\n");
            if (!m || !m.length) return 0;
            return m.length-1;
        };
        sel.fix =3D countNL(sel.obj.value.substring(0,sel.start));

    }
    return sel;
}

/**
 * Set the selection
 *
 * You need to get a selection object via DWgetSelection() first, then modi=
fy the
 * start and end properties and pass it back to this function.
 *
 * @link http://groups.drupal.org/node/1210
 * @author Andreas Gohr <andi@splitbrain.org>
 * @param {selection_class} selection  a selection object as returned by DW=
getSelection()
 */
function DWsetSelection(selection){
    if(document.getSelection){ // FF
        // what a pleasure in FF ;)
        selection.obj.setSelectionRange(selection.start,selection.end);
        if(selection.scroll) selection.obj.scrollTop =3D selection.scroll;
    } else if(document.selection) { // IE
        selection.rangeCopy.collapse(true);
        selection.rangeCopy.moveStart('character',selection.start - selecti=
on.fix);
        selection.rangeCopy.moveEnd('character',selection.end - selection.s=
tart);
        selection.rangeCopy.select();
    }
}

/**
 * Inserts the given text at the current cursor position or replaces the cu=
rrent
 * selection
 *
 * @author Andreas Gohr <andi@splitbrain.org>
 * @param {string}  text           the new text to be pasted
 * @param {selection_class}  selection     selection object returned by DWg=
etSelection
 * @param {int}     opts.startofs  number of charcters at the start to skip=
 from new selection
 * @param {int}     opts.endofs    number of characters at the end to skip =
from new selection
 * @param {boolean} opts.nosel     set true if new text should not be selec=
ted
 */
function pasteText(selection,text,opts){
    if(!opts) opts =3D {};
    // replace the content

    selection.obj.value =3D
        selection.obj.value.substring(0, selection.start) + text +
        selection.obj.value.substring(selection.end, selection.obj.value.le=
ngth);

    // set new selection
    if (is_opera) {
        // Opera replaces \n by \r\n when inserting text.
        selection.end =3D selection.start + text.replace(/\r?\n/g, '\r\n').=
length;
    } else {
        selection.end =3D selection.start + text.length;
    }

    // modify the new selection if wanted
    if(opts.startofs) selection.start +=3D opts.startofs;
    if(opts.endofs)   selection.end   -=3D opts.endofs;

    // no selection wanted? set cursor to end position
    if(opts.nosel) selection.start =3D selection.end;

    DWsetSelection(selection);
}


/**
 * Format selection
 *
 * Apply tagOpen/tagClose to selection in textarea, use sampleText instead
 * of selection if there is none.
 *
 * @author Andreas Gohr <andi@splitbrain.org>
 */
function insertTags(textAreaID, tagOpen, tagClose, sampleText){
    var txtarea =3D jQuery('#' + textAreaID)[0];

    var selection =3D DWgetSelection(txtarea);
    var text =3D selection.getText();
    var opts;

    // don't include trailing space in selection
    if(text.charAt(text.length - 1) =3D=3D ' '){
        selection.end--;
        text =3D selection.getText();
    }

    if(!text){
        // nothing selected, use the sample text and select it
        text =3D sampleText;
        opts =3D {
            startofs: tagOpen.length,
            endofs: tagClose.length
        };
    }else{
        // place cursor at the end
        opts =3D {
            nosel: true
        };
    }

    // surround with tags
    text =3D tagOpen + text + tagClose;

    // do it
    pasteText(selection,text,opts);
}

