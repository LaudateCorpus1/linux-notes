            }
        }else if(e.keyCode =3D=3D 8){ // Backspace
            // unindent lists
            var match =3D search.match(/(\n  +)([*-] ?)$/);
            if(match){
                var spaces =3D match[1].length-1;

                if(spaces > 3){ // unindent one level
                    this.value =3D this.value.substr(0,linestart)+
                                 this.value.substr(linestart+2);
                    selection.start =3D selection.start - 2;
                    selection.end   =3D selection.start;
                }else{ // delete list point
                    this.value =3D this.value.substr(0,linestart)+
                                 this.value.substr(selection.start);
                    selection.start =3D linestart;
                    selection.end   =3D linestart;
                }
