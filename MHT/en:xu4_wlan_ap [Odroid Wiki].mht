        if(selection.getLength() > 0) {
            return; //there was text selected, keep standard behavior
        }
        var search    =3D "\n"+this.value.substr(0,selection.start);
        var linestart =3D Math.max(search.lastIndexOf("\n"),
                                 search.lastIndexOf("\r")); //IE workaround
        search =3D search.substr(linestart);

        if((e.keyCode =3D=3D 13 || e.keyCode =3D=3D 10) && e.ctrlKey) { // =
Ctrl-Enter (With Chrome workaround)
