    // Toggle visibility of every tag with class "comment"
    for (i =3D 0; i < comments.length; i++){
        el =3D comments[i];
        if ( el.style.display !=3D 'none' ) {
            el.style.display =3D 'none';
        } else {
            el.style.display =3D '';
        }
    }
}

function show_toggleComments() {
    // Show edit bar item for toggling inline comments on/off only if inlin=
e comments exist on the page
    comments =3D getElementsByClassName('comment', null, document);
    if (comments.length > 0) {
