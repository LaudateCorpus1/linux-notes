        searchIsDisabled =3D true;
    }
}

function actionsMenuInit(title) {
    // Initialize action menu
    for (i =3D 0; i < document.forms.length; i++) {
        var form =3D document.forms[i];
        if (form.className =3D=3D 'actionsmenu') {
            // Check if this form needs update
            var div =3D form.getElementsByTagName('div')[0];
            var label =3D div.getElementsByTagName('label')[0];
            if (label) {
                // This is the first time: remove label and do buton.
                div.removeChild(label);
                var dobutton =3D div.getElementsByTagName('input')[0];
                div.removeChild(dobutton);
                // and add menu title
                var select =3D div.getElementsByTagName('select')[0];
                var item =3D document.createElement('option');
                item.appendChild(document.createTextNode(title));
                item.value =3D 'show';
                select.insertBefore(item, select.options[0]);
                select.selectedIndex =3D 0;
            }
        }
    }
}

