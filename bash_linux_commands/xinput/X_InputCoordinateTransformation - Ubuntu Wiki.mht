function dbw_update_search(dbw_id)
{
    var table =3D document.getElementById(dbw_id+'table');
    var cell;
    var shown;
    var i
    var cols =3D table.rows[0].cells.length;
    var filter =3D new Array();
    var dofilter =3D new Array();
    var form =3D document.forms[dbw_id+'form'];

    for (i =3D 0; i < cols; i++) {
        dofilter[i] =3D false;
        if (form[dbw_id+'filter'+i]) {
            dofilter[i] =3D true;
            filter[i] =3D form[dbw_id+'filter'+i].value;
            if (filter[i] =3D=3D '[all]')
                dofilter[i] =3D false;
            if (filter[i] =3D=3D '[empty]')
                filter[i] =3D '';
        }
    }

    for (i =3D 1; i < table.rows.length; i++) {
        var show =3D true;
        for (col =3D 0; col < cols; col++) {
            if (!dofilter[col])
                continue;

            cell =3D table.rows[i].cells[col];

            if (filter[col] =3D=3D '[notempty]') {
                if (cell.abbr =3D=3D '') {
                    show =3D false;
                    break;
                }
            } else if (filter[col] !=3D cell.abbr) {
                show =3D false;
                break;
            }
        }
        if (show)
            table.rows[i].style.display =3D '';
        else
            table.rows[i].style.display =3D 'none';
    }
}

