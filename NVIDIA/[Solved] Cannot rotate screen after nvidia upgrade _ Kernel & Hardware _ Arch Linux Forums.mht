	background: #fff;
	border-color: #cad7e1;
	color: #566579;
}

#brdtitle #brddesc, .pun .pagepost, #brdstats #onlinelist, #brdfooter #sear=
chlinks, #brdfooter #modcontrols,
#punmaint h2, #punredirect h2, #adminconsole .submittop, .pun #debugtime, .=
pun .pagelink a, .pun .pagelink * {
	border-color: #b9c5ce;
}

.pun a, .pun a:link, .pun a:visited {
